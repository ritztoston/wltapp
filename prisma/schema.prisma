datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String @id @default(cuid())
  auth0Id   String @unique
  email     String @unique
  firstName String
  lastName  String
  image     String

  createdAt String
}

model Session {
  id   String @id @default(uuid())
  ttl  Int?
  data String
}